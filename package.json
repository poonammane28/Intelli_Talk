{
  "name": "ai-chat-extension",
  "displayName": "AI Chat Extension",
  "description": "AI chatbot for VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aiChat",
          "title": "AI Chat",
          "icon": "resources/ai-icon.png"
        }
      ]
    },
    "views": {
      "aiChat": [
        {
          "id": "aiChatView",
          "name": "Chat"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}
